parametro de entrada: 
variavel vai vir pelo parametro de entrada. se não, não apresentara nada
informo dentro dos parenteses "()".
//2
#include <stdio.h>


float divisao(float x, float y, float z){
    float soma = x + y + z;
    return soma /3;
}

int main(){
    float x, y, z;
    printf("Digite o conceito A:");
    scanf("%f", &x);
    printf("Digite o conceito B:");
    scanf("%f", &y);
    printf("Digite o conceito C:");
    scanf("%f", &z);
     
    float d = divisao (x, y, z);
    
    if (d <= 4.0){
        printf("Conceito: D");
    }
    else if(4.1 && 7.0){
        printf("Conceito: C");
    }
    else if (7.1 && 9.0){
        printf("Conceito: B");
    }
    else if (9.1 && 10.0){
        printf("Conceito: A");
    }
    else {
        printf("ERRO!");
    }
    return 0;
}
//2
#include <stdio.h>

char calcularConceito(float nota1, float nota2, float nota3) {
    
    if (nota1 < 0 || nota2 < 0 || nota3 < 0) {
        return 'E';
    }

    float media = (nota1 + nota2 + nota3) / 3.0;

    if (media >= 0.0 && media < 4.0) {
        return 'D';
    } else if (media >= 4.0 && media < 7.0) {
        return 'C';
    } else if (media >= 7.0 && media < 9.0) {
        return 'B';
    } else if (media >= 9.0 && media <= 10.0) {
        return 'A';
    }

    return ' ';  
}

int main() {
    float nota1, nota2, nota3;

    printf("Digite as três notas: ");
    scanf("%f %f %f", &nota1, &nota2, &nota3);

    char conceito = calcularConceito(nota1, nota2, nota3);
    if (conceito == 'E') {
        printf("Alguma das notas é negativa.\n");
    } else {
        printf("Conceito: %c\n", conceito);
    }

    return 0;
}

//3
#include <stdio.h>


void isPrime(int num) {
        if (num <= 1) {
            printf("%d não é primo!", num);
            return 0; 
        }
        while (int i = 2; i <= num / 2 ) {
            if (num % i == 0) {
                printf("%d é primo!", num);
                return 0; 
            }
        }
    }
int main(){
    int num;
    printf("digite um numero inteiro:");
    scanf("%d", &num);
    isPrime(num);
    
    return 0;
}
